    <timbre name="Acoustic Grand Piano">
      <waveform src="triangle"/>
      <waveform src="sine">
        <processing>add</processing>
        <pitch>4.0</pitch>
        <ratio>-0.2</ratio>
      </waveform>
      <waveform src="triangle">
        <processing>mix</processing>
        <pitch>2.0</pitch>
        <ratio>0.333</ratio>
      </waveform>
    </timbre>

    <timbre name="Bright Acoustic Piano">
      <waveform src="triangle"/>
      <waveform src="sine">
        <processing>add</processing>
        <pitch>4.0</pitch>
        <ratio>-0.2</ratio>
      </waveform>
      <waveform src="sawtooth">
        <processing>mix</processing>
        <pitch>2.0</pitch>
        <ratio>0.333</ratio>
      </waveform>
    </timbre>

    <timbre name="Tubular Bells">
      <waveform src="square">
        <pitch>2.5</pitch>
      </waveform>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>3.0</pitch>
        <ratio>0.6</ratio>
      </waveform>
      <waveform src="sine">
        <processing>modulate</processing>
        <pitch>1.0</pitch>
        <ratio>0.6</ratio>
      </waveform>
    </timbre>

    <timbre name="Draw Bar Organ">
      <waveform src="triangle"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>4.0</pitch>
        <ratio>0.4</ratio>
      </waveform>
    </timbre>

    <timbre name="Rock Organ">
      <waveform src="triangle"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>3.5</pitch>
        <ratio>0.5</ratio>
      </waveform>
    </timbre>

    <timbre name="Church Organ">
      <waveform src="square"/>
      <waveform src="impulse">
        <processing>mix</processing>
        <pitch>2.0</pitch>
        <ratio>0.2</ratio>
      </waveform>
    </timbre>

    <timbre name="Reed Organ">
      <waveform src="sine"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>3.5</pitch>
        <ratio>0.2</ratio>
      </waveform>
    </timbre>

    <timbre name="Recorder">
      <waveform src="sine"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>2.0</pitch>
        <ratio>-0.2</ratio>
      </waveform>
    </timbre>

    <timbre name="Woodwind">
      <waveform src="sine"/>
      <waveform src="triangle">
        <processing>mix</processing>
        <pitch>2.0</pitch>
        <ratio>0.4</ratio>
      </waveform>
    </timbre>

    <timbre name="Violin">
      <waveform src="sawtooth"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>1.0</pitch>
        <ratio>-0.6</ratio>
      </waveform>
    </timbre>

    <timbre name="Saxophone">
      <waveform src="sawtooth"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>3.535</pitch>
        <ratio>0.2</ratio>
      </waveform>
    </timbre>

    <timbre name="Oboe">
      <waveform src="sine"/>
      <waveform src="square">
        <processing>mix</processing>
        <pitch>1.137</pitch>
        <ratio>-0.5</ratio>
      </waveform>
    </timbre>

    <timbre name="Accordion">
      <waveform src="sawtooth"/>
      <waveform src="sine">
        <processing>mix</processing>
        <pitch>3.0</pitch>
        <ratio>0.33</ratio>
      </waveform>
    </timbre>

    <timbre name="Clarinet">
      <waveform src="square"/>
      <waveform src="triangle">
        <processing>add</processing>
        <pitch>2.0</pitch>
        <ratio>-0.5</ratio>
      </waveform>
      <waveform src="triangle">
        <processing>mix</processing>
        <pitch>1.0</pitch>
        <ratio>0.3</ratio>
      </waveform>
    </timbre>

    <timbre name="Flute">
      <waveform src="brownian-noise">
         <pitch>0.50</pitch>
         <ratio>0.10</ratio>
       </waveform>
       <waveform src="sine">
         <processing>mix</processing>
       </waveform>
    </timbre>

    <timbre name="Pan Flute">
      <waveform src="brownian-noise">
         <pitch>0.50</pitch>
         <ratio>0.30</ratio>
       </waveform>
       <waveform src="triangle">
         <processing>mix</processing>
       </waveform>
    </timbre>

</soundbank>

