<aeonwave>

 <info name="Low Agogo">
 </info>

 <sound gain="1.17" frequency="440" duration="0.1">
  <waveform src="triangle" pitch="2.0"/>
  <waveform src="square" processing="modulate" ratio="0.7" pitch="3.5987"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.5</param>
    <param n="1">0.05</param>
    <param n="2">1.0</param>
    <param n="3" auto-sustain="0.1">0.75</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">1.05</param>
    <param n="1">0.005</param>
    <param n="2">0.98</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
