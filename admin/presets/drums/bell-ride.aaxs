<aeonwave>

 <info name="Bell Ride">
 </info>

 <sound gain="0.72" frequency="885" duration="0.1">
  <waveform src="square" pitch="0.985"/>
  <waveform src="sawtooth" processing="add" ratio="0.8" pitch="1.9"/>
