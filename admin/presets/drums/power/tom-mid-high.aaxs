 <sound mode="1" gain="1.97" frequency="174.614" duration="0.3">
  <layer n="0">
   <waveform src="sine"/>
   <waveform src="pink-noise" processing="add" ratio="0.1"/>
  </layer>
  <layer n="1">
   <waveform src="white-noise" ratio="0.5" pitch="0.14"/>
   <waveform src="triangle" processing="add"/>
