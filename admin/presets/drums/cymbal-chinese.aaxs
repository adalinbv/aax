<aeonwave>

 <info name="Chinese Cymbal" bank="0" program="52">
  <note polyphony="1"/>
 </info>

 <sound gain="1.56" frequency="440" duration="0.2">
  <waveform src="sawtooth" pitch="1.297"/>
  <waveform src="square" processing="modulate" pitch="2.5"/>
  <waveform src="white-noise" processing="mix" ratio="0.5" pitch="0.25"/>
  <waveform src="square" processing="modulate" pitch="2.5"/>
  <filter type="frequency">
   <slot n="0">
    <param n="0">550.0</param>
    <param n="1">0.1</param>
    <param n="2">1.2</param>
    <param n="3">2.0</param>
   </slot>
  </filter>
 </sound>

 <emitter looping="true" pan="-0.31">
  <filter type="timed-gain" release-factor="1.5">
   <slot n="0">
    <param n="0">0.648</param>
    <param n="1">0.1</param>
    <param n="2">1.11</param>
    <param n="3">1.5</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.5</param>
    <param n="1">0.1</param>
    <param n="2">1.0</param>
    <param n="3">0.1</param>
   </slot>
   <slot n="1">
    <param n="0">0.99</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

 <audioframe/>

</aeonwave>
