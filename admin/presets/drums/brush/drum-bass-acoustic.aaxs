    <param n="2">3.72</param>
    <param n="3">0.08</param>
   </slot>
   <slot n="1">
    <param n="0">1.87</param>
    <param n="1">0.3</param>
    <param n="2">0.372</param>
    <param n="3">0.2</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">2.5</param>
    <param n="1">0.03</param>
    <param n="2">1.0</param>
    <param n="3">0.05</param>
   </slot>
  </effect>
 </emitter>

 <audioframe/>

