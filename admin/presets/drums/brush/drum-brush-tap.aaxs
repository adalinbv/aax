<aeonwave>

 <info name="Snare Brush Tap" bank="40" program="38">
  <note polyphony="1"/>
 </info>

 <sound mode="2" gain="2.10" frequency="286" duration="0.5">
  <waveform src="white-noise" ratio="0.5" pitch="0.4" staticity="0.025"/>
  <filter type="bitcrusher">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.55</param>
   </slot>
  </filter>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain" release-factor="2.0">
   <slot n="0">
    <param n="0">1.3</param>
    <param n="1">0.05</param>
    <param n="2">0.9</param>
    <param n="3">0.17</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">1.1</param>
    <param n="1">0.7</param>
    <param n="2">0.9</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

 <audioframe/>

</aeonwave>
