<aeonwave>

 <info name="Electronic Snare 2" bank="24" program="40">
  <license type="Attribution-ShareAlike 4.0 International"/>
  <copyright from="2017" until="2022" by="Adalin B.V."/>
  <copyright from="2017" until="2022" by="Erik Hofman"/>
  <contact author="Erik Hofman" website="aeonwave.xyz"/>
  <note polyphony="1"/>
 </info>

 <sound mode="2" gain="0.79" frequency="220" duration="0.5">
  <waveform src="triangle" ratio="0.25" pitch="1.3"/>
  <waveform src="triangle" processing="add" ratio="0.4"/>
  <waveform src="white-noise" processing="add" ratio="0.1" pitch="0.14"/>
 </sound>

 <emitter looping="true">
  <filter type="bitcrusher" src="envelope">
   <slot n="0">
    <param n="0">0.04</param>
    <param n="1">0.1</param>
    <param n="2">0.3</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
  <filter type="timed-gain" src="envelope" release-time="0.06">
   <slot n="0">
    <param n="0">4.24</param>
    <param n="1">0.95</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
  <effect type="timed-pitch" src="envelope">
   <slot n="0">
    <param n="0">1.2</param>
    <param n="1">0.01</param>
    <param n="2">1.2</param>
    <param n="3">0.01</param>
   </slot>
   <slot n="1">
    <param n="0">1.03</param>
    <param n="1">0.4</param>
    <param n="2">0.98</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
