    <param n="1">0.07</param>
    <param n="2">0.5</param>
    <param n="3">0.15</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.5</param>
    <param n="1">0.02</param>
    <param n="2">7.0</param>
    <param n="3">0.01</param>
   </slot>
   <slot n="1">
    <param n="0">0.85</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
