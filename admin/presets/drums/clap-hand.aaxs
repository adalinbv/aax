<aeonwave>

 <info name="Handclap">
 </info>

 <sound gain="0.39" frequency="220" duration="0.2">
  <waveform src="white-noise" pitch="0.25"/>
  <filter type="frequency" src="24db">
   <slot n="0">
    <param n="0" pitch="18.0">3960.0</param>
    <param n="1">0.6</param>
    <param n="2">0.1</param>
    <param n="3">2.5</param>
