  <waveform src="sine" processing="modulate" ratio="0.7" pitch="11.7587"/>
  <waveform src="sine" processing="modulate" pitch="17.7587"/>
  <filter type="frequency" src="24db">
   <slot n="0">
    <param n="0" pitch="4.0">3520.0</param>
