<aeonwave>

 <instrument program="54" bank="0" name="Synth Vox">
  <note polyphony="8" min="48" max="84" step="12"/>
 </instrument>

 <sound frequency="220" duration="0.50">
  <waveform src="triangle" pitch="0.98"/>
  <waveform src="sawtooth" processing="modulate" ratio="0.5" pitch="8.0"/>
  <waveform src="square" processing="add" ratio="0.3" pitch="1.0"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.2</param>
    <param n="1" factor="velocity">0.2</param>
    <param n="2" factor="soften">1.2</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">1.00</param>
    <param n="1">inf</param>
    <param n="2">0.9</param>
    <param n="3">0.5</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
