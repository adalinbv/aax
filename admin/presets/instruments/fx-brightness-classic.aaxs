  <note polyphony="4" min="36" max="96" step="12"/>
  <sound version="1.1"/>
 </info>

 <sound gain="0.423" frequency="440" duration="0.7">
  <layer n="0">
   <waveform src="square" voices="3" spread="0.93" phasing="true"/>
   <waveform src="pure-impulse" processing="add" ratio="0.15" pitch="3.0"/>
  </layer>
  <layer n="1">
   <waveform src="square" voices="3" spread="0.93"/>
   <waveform src="impulse" processing="add" ratio="0.35" pitch="3.0"/>
  </layer>
 </sound>

 <emitter looping="true">
  <filter type="dynamic-layer" src="inverse-timed|exponential">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">2.5</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
  <filter type="bitcrusher">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">0.01</param>
    <param n="2">0.0</param>
    <param n="3">0.2</param>
   </slot>
  </filter>
  <filter type="timed-gain" src="envelope" release-time="0.5">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1" type="msec" min="10.0" max="5.0" tune="-1.2e-06">0.01</param>
    <param n="2">0.412</param>
    <param n="3">0.025</param>
   </slot>
   <slot n="1">
