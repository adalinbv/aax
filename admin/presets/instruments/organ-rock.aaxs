  <effect type="phasing" src="true">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.49</param>
   </slot>
  </effect>
 </audioframe>

</aeonwave>
