<aeonwave>

 <info name="Helicopter" bank="0" program="0">
  <license type="Attribution-ShareAlike 4.0 International"/>
  <copyright from="2017" until="2025" by="Adalin B.V."/>
  <copyright from="2017" until="2025" by="Erik Hofman"/>
  <contact author="Erik Hofman" website="aeonwave.xyz"/>
  <note polyphony="1" min="21" max="40" pitch-fraction="0.5"/>
  <sound version="1.1"/>
 </info>

 <sound mode="1" frequency="55" duration="5.0">
  <layer n="0">
   <waveform src="pink-noise"/>
   <waveform src="cycloid" processing="mix" ratio="0.4" pitch="0.75"/>
   <waveform src="sawtooth" processing="modulate" ratio="0.133" pitch="0.25"/>
  </layer>
 </sound>

 <emitter gain="420.5" looping="true">
  <filter type="timed-gain" src="envelope" release-time="2.0">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">0.2</param>
    <param n="2">0.75</param>
    <param n="3">1.5</param>
   </slot>
   <slot n="1">
    <param n="0">1000.0</param>
    <param n="1">1.0</param>
    <param n="2">2.0</param>
    <param n="3">0.0</param>
   </slot>
   <slot n="2">
    <param n="0">0.0</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.75</param>
    <param n="1">2.0</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
