<aeonwave>

 <instrument program="63" bank="0" name="Synth Brass 1">
  <note polyphony="2" min="35" max="96" step="12"/>
 </instrument>

 <sound frequency="220" duration="0.25">
  <waveform src="impulse"/>
  <waveform src="impulse" processing="add" pitch="1.407"/>
  <waveform src="impulse" processing="add" pitch="0.993"/>
  <effect type="distortion">
   <slot n="0">
    <param n="0">0.8</param>
    <param n="1">0.0</param>
    <param n="2">0.2</param>
    <param n="3">1.0</param>
   </slot>
