   <slot n="0">
    <param n="0" pitch="1.0">220</param>
    <param n="1">1.2</param>
    <param n="2">0.0</param>
    <param n="3">1.5</param>
   </slot>
   <slot n="1">
    <param n="0" pitch="20">4400</param>
    <param n="3">0.4</param>
   </slot>
  </filter>
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.20</param>
    <param n="1" factor="velocity">0.05</param>
    <param n="2" factor="soften">1.00</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.80</param>
    <param n="1">2.5</param>
    <param n="2">0.50</param>
    <param n="3">1.50</param>
   </slot>
  </filter>
  <effect type="chorus" src="envelope" optional="true">
   <slot n="0">
    <param n="0">0.6</param>
    <param n="1">0.05</param>
    <param n="2">0.27</param>
    <param n="3">0.23</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
