<aeonwave>

 <info name="Fiddle" bank="0" program="110">
  <note polyphony="4" min="55" max="96" step="12"/>
 </info>

 <sound gain="3.50" frequency="440" duration="0.75">
  <waveform src="sawtooth" ratio="0.6"/>
  <waveform src="square" processing="modulate" pitch="3.0"/>
  <waveform src="impulse" processing="mix" ratio="0.15" pitch="0.9899"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.4</param>
    <param n="1">0.15</param>
    <param n="2">1.0</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.9</param>
    <param n="1">inf</param>
    <param n="2">0.9</param>
    <param n="3">0.2</param>
   </slot>
  </filter>
 </emitter>

 <audioframe>
  <filter type="equalizer" optional="true">
   <slot n="0">
    <param n="0">390.0</param>
    <param n="1">1.2</param>
    <param n="2">1.0</param>
    <param n="3">2.1</param>
   </slot>
   <slot n="1">
    <param n="0">750.0</param>
    <param n="1">1.0</param>
    <param n="2">0.6</param>
    <param n="3">4.1</param>
   </slot>
  </filter>
 </audioframe>

</aeonwave>
