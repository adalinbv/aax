  <waveform src="triangle" processing="modulate" pitch="1.999"/>
  <waveform src="triangle" processing="add" ratio="0.15" pitch="7.989"/>
  <filter type="frequency">
   <slot n="0">
    <param n="0" pitch="1.1">60.5</param>
    <param n="1">0.0</param>
    <param n="2">1.0</param>
    <param n="3">2.0</param>
   </slot>
  </filter>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain" release-factor="2.0">
   <slot n="0">
    <param n="0">1.15</param>
    <param n="1">0.01</param>
    <param n="2">0.512</param>
    <param n="3">0.02</param>
   </slot>
