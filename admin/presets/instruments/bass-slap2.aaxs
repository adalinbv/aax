    <param n="0">0.779</param>
    <param n="1">0.5</param>
    <param n="2">0.689</param>
    <param n="3">0.689</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.2</param>
    <param n="1">0.02</param>
    <param n="2">1.16</param>
    <param n="3">0.01</param>
   </slot>
