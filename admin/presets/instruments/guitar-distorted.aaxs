<aeonwave>

 <instrument program="30" bank="0">
  <note polyphony="6" min="40" max="86" step="12"/>
 </instrument>

 <sound name="Distorted Electric Guitar" frequency="220" duration="0.10">
  <waveform src="sawtooth" ratio="0.9"/>
  <waveform src="square" processing="add" ratio="0.6" pitch="2.0"/>
  <waveform src="sawtooth" processing="modulate" ratio="0.7" pitch="5.0"/>
  <filter type="frequency" src="true">
   <slot n="0">
    <param n="0" pitch="8.0">1760</param>
    <param n="1">0.5</param>
    <param n="2">0.0</param>
    <param n="3">4.5</param>
   </slot>
  </filter>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.00</param>
    <param n="1" factor="velocity">0.01</param>
    <param n="2" factor="soften">1.00</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.60</param>
    <param n="1">inf</param>
    <param n="2">0.60</param>
    <param n="3">0.6</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0" factor="soften">1.15</param>
    <param n="1" factor="velocity">0.03</param>
    <param n="2">0.95</param>
    <param n="3">0.03</param>
   </slot>
   <slot n="1">
    <param n="0">1.0</param>
    <param n="1">4.5</param>
    <param n="2">0.975</param>
   </slot>
  </effect>
 </emitter>

 <audioframe>
  <effect type="distortion">
   <slot n="0">
    <param n="0">1.5</param>
    <param n="1">0.8</param>
    <param n="2">0.5</param>
    <param n="3">0.2</param>
   </slot>
  </effect>
  <effect type="chorus" src="envelope" optional="true">
   <slot n="0">
    <param n="0">0.6</param>
    <param n="1">0.1</param>
    <param n="2">0.5</param>
    <param n="3">0.2</param>
   </slot>
  </effect>
  <filter type="equalizer" optional="true">
   <slot n="0">
    <param n="0">160</param>
    <param n="1">0.5</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
   <slot n="1">
    <param n="0">4000</param>
    <param n="1">1.0</param>
    <param n="2">0.5</param>
    <param n="3">1.6</param>
   </slot>
  </filter>
 </audioframe>

</aeonwave>
