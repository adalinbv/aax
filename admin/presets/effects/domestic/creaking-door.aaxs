  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.97</param>
    <param n="1">2.2</param>
    <param n="2">0.87</param>
    <param n="3">0.4</param>
   </slot>
   <slot n="1">
    <param n="0">0.9</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
