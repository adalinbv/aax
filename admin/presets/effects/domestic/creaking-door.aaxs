  <filter type="frequency" src="24db">
   <slot n="0">
    <param n="0" pitch="1.0">330.0</param>
    <param n="1">0.0</param>
    <param n="2">0.75</param>
    <param n="3">7.5</param>
   </slot>
  </filter>
  <filter type="frequency" src="48db">
   <slot n="0">
    <param n="0" pitch="20.0">6600.0</param>
    <param n="1">1.0</param>
    <param n="2">0.25</param>
    <param n="3">5.5</param>
   </slot>
  </filter>
  <filter type="frequency" src="12db">
   <slot n="0">
