<aeonwave>

 <info name="Electric Current">
  <license type="Attribution-ShareAlike 4.0 International"/>
  <copyright from="2017" until="2022" by="Adalin B.V."/>
  <copyright from="2017" until="2022" by="Erik Hofman"/>
  <contact author="Erik Hofman" website="aeonwave.xyz"/>
  <note polyphony="1"/>
 </info>

 <sound gain="1.18" frequency="550">
  <waveform src="pink-noise" pitch="0.25"/>
  <waveform src="sawtooth" processing="mix" ratio="0.7"/>
 </sound>

 <emitter looping="true">
  <filter type="dynamic-gain" src="randomness">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">1.0</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
 </emitter>

 <audioframe/>

 <mixer/>

</aeonwave>
