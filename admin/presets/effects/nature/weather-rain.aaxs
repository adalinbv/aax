 <sound gain="0.81" frequency="220" duration="3.0">
  <layer ratio="0.75">
   <waveform src="white-noise" pitch="0.2" staticity="0.04"/>
   <waveform src="pink-noise" processing="modulate" ratio="30.0"/>
   <waveform src="pink-noise" processing="modulate" ratio="8.0" pitch="2.0"/>
   <filter type="frequency">
    <slot n="0">
     <param n="0" pitch="0.5">110.0</param>
     <param n="1">0.0</param>
     <param n="2">1.0</param>
     <param n="3">1.0</param>
    </slot>
   </filter>
  </layer>
  <layer>
   <waveform src="pink-noise" pitch="0.2" staticity="0.08"/>
   <waveform src="pink-noise" processing="modulate" ratio="30.0"/>
   <filter type="frequency">
    <slot n="0">
     <param n="0" pitch="0.3">66.0</param>
     <param n="1">0.0</param>
     <param n="2">1.0</param>
     <param n="3">0.0</param>
    </slot>
   </filter>
  </layer>
 </sound>

 <emitter looping="true">
  <filter type="dynamic-layer" src="inverse-envelope">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">3.0</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">1.5</param>
    <param n="2">3.0</param>
    <param n="3">inf</param>
   </slot>
   <slot n="1">
    <param n="0">3.0</param>
    <param n="1">0.5</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
