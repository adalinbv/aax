<aeonwave>

 <info>
  <license type="Attribution-ShareAlike 4.0 International"/>
  <copyright from="2017" until="2019" by="Erik Hofman"/>
  <copyright from="2017" until="2019" by="Adalin B.V."/>
  <note polyphony="1"/>
 </info>

 <sound gain="0.93" frequency="220" duration="0.3">
  <waveform src="white-noise" ratio="0.25" pitch="0.5"/>
  <waveform src="sawtooth" processing="modulate" ratio="0.33"/>
  <waveform src="square" processing="mix" ratio="0.4" voices="3"/>
  <waveform src="triangle" processing="add" ratio="0.2" pitch="1.5"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.4</param>
    <param n="1">inf</param>
    <param n="2">1.4</param>
    <param n="3">0.3</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">0.225</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

 <audioframe>
  <filter type="equalizer" optional="true">
   <slot n="0">
    <param n="0">125.0</param>
    <param n="1">1.2</param>
    <param n="2">0.7</param>
    <param n="3">5.0</param>
   </slot>
   <slot n="1">
    <param n="0">750.0</param>
    <param n="1">0.7</param>
    <param n="2">0.0</param>
    <param n="3">3.0</param>
   </slot>
  </filter>
  <effect type="chorus" src="envelope" optional="true">
   <slot n="0">
    <param n="0">0.7</param>
    <param n="1">0.2</param>
    <param n="2">0.4</param>
    <param n="3">0.1</param>
   </slot>
   <slot n="1">
    <param n="0">125.0</param>
    <param n="1">3750.0</param>
    <param n="2">0.5</param>
    <param n="3">3.0</param>
   </slot>
  </effect>
 </audioframe>

</aeonwave>
