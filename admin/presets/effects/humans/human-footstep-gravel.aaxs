  <effect type="timed-pitch" src="envelope">
   <slot n="0">
    <param n="0">1.2</param>
    <param n="1">0.03</param>
    <param n="2">0.25</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </emitter>

