 <sound gain="1.02" frequency="220" duration="0.3">
  <layer n="0">
   <waveform src="white-noise" pitch="0.25"/>
   <filter type="frequency" src="24db">
    <slot n="0">
     <param n="0" pitch="11.0">2420.0</param>
