   <slot n="0">
    <param n="0">0.5</param>
    <param n="1">0.02</param>
    <param n="2">7.0</param>
    <param n="3">0.01</param>
   </slot>
   <slot n="1">
    <param n="0">0.85</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.44</param>
    <param n="1">0.07</param>
    <param n="2">0.837</param>
    <param n="3">0.15</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
