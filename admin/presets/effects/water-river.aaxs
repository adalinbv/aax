<aeonwave>

 <sound name="River" program="123" bank="4" frequency="220" duration="2.9">
  <waveform src="white-noise" ratio="15.0" pitch="0.25"/>
  <waveform src="brownian-noise" processing="add" ratio="15.0" pitch="0.05" staticity="0.9"/>
 </sound>

 <emitter looping="true">
  <filter type="frequency" src="inverse-envelope">
   <slot n="0">
    <param n="0">330</param>
    <param n="1">0.2</param>
    <param n="2">0.3</param>
    <param n="3">10.5</param>
   </slot>
   <slot n="1">
    <param n="0">17500</param>
    <param n="3">2.0</param>
   </slot>
  </filter>
 </emitter>

 <audioframe>
  <filter type="frequency" src="12db">
   <slot n="0">
    <param n="0">2200</param>
    <param n="1">1.0</param>
    <param n="2">0.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
 </audioframe>

</aeonwave>
