<aeonwave>

 <sound name="Voice Oohs" frequency="220">
  <waveform src="pink-noise"/>
  <waveform src="pink-noise" processing="modulate" ratio="8.0"/>
  <filter type="frequency">
   <slot n="0">
    <param n="0" pitch="10.0">2200</param>
    <param n="1">0.6</param>
    <param n="2">0.1</param>
    <param n="3">16.0</param>
   </slot>
  </filter>
  <waveform src="sine" processing="mix" ratio="0.8" pitch="0.4894"/>
  <waveform src="sine" processing="add" ratio="0.3" pitch="0.987"/>
  <waveform src="sine" processing="modulate" ratio="1.0" pitch="3.01"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.70</param>
    <param n="1" factor="velocity">0.20</param>
    <param n="2" factor="soften">1.00</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.90</param>
    <param n="1">inf</param>
    <param n="2">0.80</param>
    <param n="3">0.20</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0" factor="soften">0.92</param>
    <param n="1" factor="velocity">0.15</param>
    <param n="2">1.0</param>
    <param n="3">0.1</param>
   </slot>
   <slot n="1">
    <param n="0">1.0</param>
    <param n="1">4.5</param>
    <param n="2">0.975</param>
   </slot>
  </effect>
 </emitter>

 <audioframe>
  <effect type="chorus" src="envelope">
   <slot n="0">
    <param n="0">0.8</param>
    <param n="1">0.1</param>
    <param n="2">0.1</param>
    <param n="3">0.4</param>
   </slot>
  </effect>
 </audioframe>

</aeonwave>
