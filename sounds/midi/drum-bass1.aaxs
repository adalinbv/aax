<aeonwave>

 <sound name="Bass Drum 1" frequency="22">
  <waveform src="sine" ratio="0.8"/>
  <waveform src="triangle" processing="add" ratio="0.8" pitch="3.0"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">0.00</param>
    <param n="1">0.01</param>
    <param n="2">1.20</param>
    <param n="3">0.08</param>
   </slot>
   <slot n="1">
    <param n="0">0.50</param>
    <param n="1">0.30</param>
    <param n="2">0.10</param>
    <param n="3">0.40</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0">3.00</param>
    <param n="1">0.03</param>
    <param n="2">1.00</param>
    <param n="3">0.05</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
