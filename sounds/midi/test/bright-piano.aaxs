<?xml version="1.0"?>

<aeonwave>

 <sound name="Bright Acoustic Piano" frequency="55">
  <waveform src="triangle"/>
  <waveform src="sine" processing="add" pitch="4.0" ration="-0.2"/>
  <waveform src="sawtooth" processing="mix" pitch="2.0" ration="0.3333"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">0.00</param>
    <param n="1" factor="velocity">0.01</param>
    <param n="2" factor="soften">1.20</param>
    <param n="3">0.02</param>
   </slot>
   <slot n="1">
    <param n="0">0.40</param>
    <param n="1">2.80</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
