<?xml version="1.0"?>

<aeonwave>

 <sound name="Reed Organ" frequency="220">
  <waveform src="sawtooth" ratio="0.2"/>
  <waveform src="sawtooth" processing="mix" pitch="0.5" ratio="0.2"/>
  <waveform src="square" processing="mix" pitch="0.125" ratio="0.2"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">0.00</param>
    <param n="1" factor="velocity">0.05</param>
    <param n="2" factor="soften">1.00</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.90</param>
    <param n="1">inf</param>
    <param n="2">0.80</param>
    <param n="3">0.20</param>
   </slot>
  </filter>
  <effect src="chrosus" src="triangle">
   <slot n="0">
    <p1>0.4</p1>
    <p2>0.3</p2>
    <p3>0.1</p3>
    <p4>0.2</p4>
   </slot>
  </effect>
 </emitter>

</aeonwave>
