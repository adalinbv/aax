<aeonwave>

 <sound name="Gunshot" frequency="220">
  <waveform src="white-noise" pitch="0.25"/>
  <waveform src="white-noise" processing="modulate" ratio="8.0" pitch="0.25"/>
  <filter type="frequency" src="24db">
   <slot n="0">
    <param n="0">3500</param>
    <param n="1">1.0</param>
    <param n="2">0.1</param>
    <param n="3">1.5</param>
   </slot>
  </filter>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.4</param>
    <param n="1" factor="velocity">0.1</param>
    <param n="2" factor="soften">1.40</param>
    <param n="3">0.6</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0" factor="soften">1.5</param>
    <param n="1" factor="velocity">0.04</param>
    <param n="2">0.9</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
