<?xml version="1.0"?>

<aeonwave>

 <sound name="Acoustic Grand Piano" frequency="55">
  <waveform src="square"/>
  <waveform src="triangle" processing="add" pitch="17.999" ratio="0.4"/>
  <waveform src="square" processing="modulate" pitch="2.0" ratio="1.5"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">0.00</param>
    <param n="1" factor="velocity">0.01</param>
    <param n="2" factor="soften">1.20</param>
    <param n="3">0.02</param>
   </slot>
   <slot n="1">
    <param n="0">0.50</param>
    <param n="1">2.80</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
