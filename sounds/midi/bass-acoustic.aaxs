<aeonwave>

 <sound name="Acoustic Bass" frequency="47">
  <waveform src="square"/>
  <waveform src="triabgle" processing="mix" pitch="1.999" ratio="0.75"/>
  <waveform src="triangle" processing="modulate" pitch="0.999" ratio="2.0"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">2.00</param>
    <param n="1" factor="velocity">0.04</param>
    <param n="2" factor="soften">1.20</param>
    <param n="3">0.05</param>
   </slot>
   <slot n="1">
    <param n="0">0.80</param>
    <param n="1">2.50</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0" factor="soften">1.2</param>
    <param n="1" factor="velocity">0.02</param>
    <param n="2">1.0</param>
    <param n="3">0.1</param>
   </slot>
   <slot n="1">
    <param n="0">1.0</param>
    <param n="1">2.5</param>
    <param n="2">0.975</param>
   </slot>
  </effect>

 </emitter>

</aeonwave>
