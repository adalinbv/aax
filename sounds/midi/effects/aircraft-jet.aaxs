   <slot n="0">
    <param n="0">80</param>
    <param n="1">0.0</param>
    <param n="2">1.0</param>
    <param n="3">0.6</param>
   </slot>
  </filter>
  <filter type="frequency">
   <slot n="0">
    <param n="0">2300</param>
    <param n="1">1.0</param>
    <param n="2">0.0</param>
    <param n="3">1.2</param>
   </slot>
  </filter>
  <effect type="phasing">
   <slot n="0">
    <param n="0">0.3</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.32</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
