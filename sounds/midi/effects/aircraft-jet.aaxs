<aeonwave>

 <sound name="Jet Aircraft" frequency="1900" duration="3.33">
  <waveform src="square" ratio="0.5" pitch="0.5"/>
  <waveform src="square" processing="modulate" ratio="1.5" pitch="2.0"/>
  <waveform src="white-noise" processing="mix" ratio="0.75" pitch="0.09"/>
