<aeonwave>

 <sound name="Telephone Ring" frequency="1250">
  <waveform src="square" pitch="0.985"/>
  <waveform src="sawtooth" processing="mix" pitch="2.9"/>
  <waveform src="sine" processing="modulate" ratio="1.0" pitch="6.3"/>
 </sound>

 <emitter looping="true">
  <filter type="dynamic_gain" src="sawtooth">
   <slot n="0">
    <param n="1">15.0</param>
    <param n="2">2.0</param>
   </slot>
  </filter>
  <filter type="envelope" repeat="1">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">0.01</param>
    <param n="2">2.0</param>
    <param n="3">1.2</param>
   </slot>
   <slot n="1">
    <param n="0">2.0</param>
    <param n="1">0.1</param>
    <param n="2">0.04</param>
    <param n="3">1.8</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
