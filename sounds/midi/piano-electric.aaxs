<aeonwave>

 <sound name="Electric Grand Piano" frequency="220">
  <waveform src="sine"/>
  <waveform src="sine" processing="mix" ratio="0.2" pitch="10.0"/>
  <waveform src="sine" processing="modulate" ratio="1.2" pitch="2.0"/>
 </sound>

 <emitter looping="true">
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.50</param>
    <param n="1" factor="velocity">0.01</param>
    <param n="2" factor="soften">1.10</param>
    <param n="3">0.09</param>
   </slot>
   <slot n="1">
    <param n="0">1.00</param>
    <param n="1" factor="sustain">3.00</param>
   </slot>
  </filter>
  <effect type="timed-pitch">
   <slot n="0">
    <param n="0" factor="soften">0.0</param>
    <param n="1" factor="velocity">0.02</param>
    <param n="2">1.0</param>
    <param n="3">0.1</param>
   </slot>
   <slot n="1">
    <param n="0">1.0</param>
    <param n="1">0.5</param>
    <param n="2">0.995</param>
   </slot>
  </effect>
 </emitter>

</aeonwave>
